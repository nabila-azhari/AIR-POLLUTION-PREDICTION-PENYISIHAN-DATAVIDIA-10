    "import pandas as pd\n",
    "from pathlib import Path\n",
    "\n",
    "# =====================\n",
    "# PATH\n",
    "# =====================\n",
    "BASE_DIR = Path.cwd()\n",
    "INPUT_CSV = BASE_DIR / \"ispu_all_years_interpolated.csv\"\n",
    "OUTPUT_CSV = BASE_DIR / \"ispu_all_years_duplicate_handled.csv\"\n",
    "\n",
    "# =====================\n",
    "# LOAD DATA\n",
    "# =====================\n",
    "df = pd.read_csv(INPUT_CSV)\n",
    "\n",
    "# =====================\n",
    "# PREPARE DATA\n",
    "# =====================\n",
    "\n",
    "# pastikan kolom max numerik\n",
    "df[\"max\"] = pd.to_numeric(df[\"max\"], errors=\"coerce\")\n",
    "\n",
    "# ranking kategori (semakin besar = semakin buruk)\n",
    "kategori_rank = {\n",
    "    \"BAIK\": 1,\n",
    "    \"SEDANG\": 2,\n",
    "    \"TIDAK SEHAT\": 3,\n",
    "    \"SANGAT TIDAK SEHAT\": 4,\n",
    "    \"BERBAHAYA\": 5\n",
    "}\n",
    "df[\"kategori_rank\"] = df[\"kategori\"].map(kategori_rank)\n",
    "\n",
    "# hitung jumlah polutan terisi\n",
    "polutan_cols = [\n",
    "    \"pm_sepuluh\",\n",
    "    \"sulfur_dioksida\",\n",
    "    \"karbon_monoksida\",\n",
    "    \"ozon\",\n",
    "    \"nitrogen_dioksida\"\n",
    "]\n",
    "df[\"filled_polutan\"] = df[polutan_cols].notna().sum(axis=1)\n",
    "\n",
    "# =====================\n",
    "# DEDUPLICATION LOGIC\n",
    "# =====================\n",
    "\n",
    "# sort sesuai rule\n",
    "df_sorted = df.sort_values(\n",
    "    by=[\"id\", \"kategori_rank\", \"max\", \"filled_polutan\"],\n",
    "    ascending=[True, False, False, False]\n",
    ")\n",
    "\n",
    "# ambil 1 baris terbaik per ID\n",
    "df_dedup = df_sorted.drop_duplicates(\n",
    "    subset=[\"id\"],\n",
    "    keep=\"first\"\n",
    ")\n",
    "\n",
    "# =====================\n",
    "# CLEANUP\n",
    "# =====================\n",
    "df_dedup = df_dedup.drop(columns=[\"kategori_rank\", \"filled_polutan\"])\n",
    "\n",
    "# =====================\n",
    "# SAVE OUTPUT\n",
    "# =====================\n",
    "df_dedup.to_csv(OUTPUT_CSV, index=False)\n",
    "\n",
    "print(\"âœ… Duplicate handling selesai\")\n",
    "print(f\"ðŸ“„ Output saved to: {OUTPUT_CSV}\")\n",
    "print(f\"ðŸ“‰ Rows before : {len(df)}\")\n",
    "print(f\"ðŸ“ˆ Rows after  : {len(df_dedup)}\")\n"
   ]
  }
